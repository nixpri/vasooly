{
  "expo": {
    "name": "vasooly",
    "slug": "vasooly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CADisableMinimumFrameDurationOnPhone": true,
        "LSApplicationQueriesSchemes": [
          "whatsapp"
        ]
      },
      "bundleIdentifier": "com.anonymous.vasooly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.vasooly"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store",
      "./plugins/withAndroidHighRefreshRate",
      [
        "react-native-share",
        {
          "ios": [
            "whatsapp"
          ],
          "android": [
            "com.whatsapp",
            "com.whatsapp.w4b"
          ],
          "enableBase64ShareAndroid": true
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "9c76610f-8704-417b-af9b-a2e141d32ce3"
      }
    }
  }
}
