{
  "name": "vasooly",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "@react-native-community/slider": "^5.0.1",
    "@shopify/flash-list": "^2.1.0",
    "@shopify/react-native-skia": "^2.3.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.13",
    "expo-contacts": "~15.0.9",
    "expo-device": "^8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-haptics": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "moti": "^0.30.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-svg": "^15.14.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-preset-expo": "^54.0.4",
    "eslint": "^9.37.0",
    "eslint-config-expo": "^10.0.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.12",
    "react-native-worklets": "^0.6.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
